{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "mppps/searchbar",
	"title": "Search bar",
	"category": "mappps",
	"description": "Contains search bar.",
	"textdomain": "mappps",
	"parent": ["mppps/mappps-blocks"],
	"ancestor": ["mppps/sidebar"],
	"usesContext": ["mppps/blockId"],
	"supports": {
		"reusable": false,
		"html": false,
		"layout": false,
		"spacing": {
			"padding": true,
			"margin": true
		},
		"interactivity": {
			"clientNavigation": true
		}
	},
	"attributes": {
		"style": {
			"type": "object",
			"default": {
				"spacing": {
					"margin": "2rem"
				}
			}
		},
		"limitedSearch": {
			"type": "boolean",
			"default": false
		}
	},
	"viewScript": "file:./assets/view.js",
	"editorScript": "file:./assets/index.jsx",
	"editorStyle": "file:./assets/index.css",
	"style": "file:./assets/style-index.css"
}
